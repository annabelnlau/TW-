{
  "manifest_version": 2,
  "name": "TW:",
  "description": "This extension creates an alert to warn a user of potentially triggering content.",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ]
}